# Naver OAuth Configuration
NAVER_CLIENT_ID=your_naver_client_id_here
NAVER_CLIENT_SECRET=your_naver_client_secret_here

# Security Settings
# State timeout in seconds (default: 600 = 10 minutes)
OAUTH_STATE_MAX_AGE_SECONDS=600

# Require HTTPS for redirect URIs (set to true in production)
OAUTH_REQUIRE_HTTPS=false

# Allow localhost redirect URIs (set to false in production)
OAUTH_ALLOW_LOCALHOST=true

# Application Settings
SERVER_PORT=8080
LOGGING_LEVEL_ROOT=INFO
LOGGING_LEVEL_COM_EXAMPLE=DEBUG
